            <ExtraType Name="Action_SpawnArmy" DataType="QuestBehaviourTreeNode_Action_SpawnArmy, Assembly-CSharp" />
            <ExtraType Name="Action_SelectVictoryQuestLocations" DataType="QuestBehaviourTreeNode_Action_SelectVictoryQuestLocations, Assembly-CSharp" />
            <ExtraType Name="Action_StartTimer" DataType="QuestBehaviourTreeNode_Action_StartTimer, Assembly-CSharp" />
            <ExtraType Name="Action_TransferResource" DataType="QuestBehaviourTreeNode_Action_TransferResource, Assembly-CSharp" />
            <ExtraType Name="Action_TriggerQuest" DataType="QuestBehaviourTreeNode_Action_TriggerQuest, Assembly-CSharp" />
            <ExtraType Name="Action_RemoveQuestMarker" DataType="QuestBehaviourTreeNode_Action_RemoveQuestMarker, Assembly-CSharp" />
            <ExtraType Name="Action_RemoveWorldEffect" DataType="QuestBehaviourTreeNode_Action_RemoveWorldEffect, Assembly-CSharp" />
            <ExtraType Name="Action_UpdateQuestMarker" DataType="QuestBehaviourTreeNode_Action_UpdateQuestMarker, Assembly-CSharp" />
            <!--Deprecated - will raise a warning if used-->
            <ExtraType Name="Action_UpdateArmyObjective" DataType="QuestBehaviourTreeNode_Action_UpdateArmyObjective, Assembly-CSharp" />
            <ExtraType Name="Action_UpdateQuest" DataType="QuestBehaviourTreeNode_Action_UpdateQuest, Assembly-CSharp" />
            <ExtraType Name="Action_UpdateStep" DataType="QuestBehaviourTreeNode_Action_UpdateStep, Assembly-CSharp" />
