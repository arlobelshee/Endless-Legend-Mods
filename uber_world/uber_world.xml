      </DatabasePlugin>
      <DatabasePlugin DataType="DepartmentOfPlanificationAndDevelopment+ConstructibleElement, Assembly-CSharp">
        <ExtraTypes>
          <ExtraType DataType="BoosterDefinition, Assembly-CSharp" />
          <ExtraType DataType="EmpirePlanDefinition, Assembly-CSharp" />
        </ExtraTypes>
