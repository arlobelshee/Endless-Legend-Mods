            <ExtraType Name="Action_Tutorial_HideInstructionPanel" DataType="QuestBehaviourTreeNode_Action_Tutorial_HideInstructionPanel, Assembly-CSharp" />
            <ExtraType Name="Action_Tutorial_SetValue" DataType="QuestBehaviourTreeNode_Action_Tutorial_SetValue, Assembly-CSharp" />

            <!-- Decorators -->
            <ExtraType Name="Decorator_ArmyDestroyed" DataType="QuestBehaviourTreeNode_Decorator_ArmyDestroyed, Assembly-CSharp" />
            <ExtraType Name="Decorator_BeginTurn" DataType="QuestBehaviourTreeNode_Decorator_BeginTurn, Assembly-CSharp" />
            <ExtraType Name="Decorator_BoosterActivated" DataType="QuestBehaviourTreeNode_Decorator_BoosterActivated, Assembly-CSharp" />
            <ExtraType Name="Decorator_CaptureCity" DataType="QuestBehaviourTreeNode_Decorator_CaptureCity, Assembly-CSharp" />
            <ExtraType Name="Decorator_CollectOrbs" DataType="QuestBehaviourTreeNode_Decorator_CollectOrbs, Assembly-CSharp" />
            <ExtraType Name="Decorator_Colonize" DataType="QuestBehaviourTreeNode_Decorator_Colonize, Assembly-CSharp" />
            <ExtraType Name="Decorator_ConstructionEnded" DataType="QuestBehaviourTreeNode_Decorator_ConstructionEnded, Assembly-CSharp" />
