      </DatabasePlugin>
      <DatabasePlugin DataType="WorldFormation, Assembly-CSharp">
        <FilePath>Simulation/WorldFormations.xml</FilePath>
      </DatabasePlugin>

      <!-- Mapping -->

      <DatabasePlugin DataType="Amplitude.Unity.View.Mapping.Xml.XmlMapping, Assembly-CSharp-firstpass">
        <FilePath>Mapping/Mapping.xml</FilePath>
      </DatabasePlugin>
      <DatabasePlugin DataType="AnomalyTypeMapping, Assembly-CSharp">
        <FilePath>Mapping/AnomalyTypeMapping[*].xml</FilePath>
      </DatabasePlugin>
      <DatabasePlugin DataType="BiomeTypeMapping, Assembly-CSharp">
        <FilePath>Mapping/BiomeTypeMapping[*].xml</FilePath>
      </DatabasePlugin>
