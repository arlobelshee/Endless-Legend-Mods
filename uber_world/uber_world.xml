            <ExtraType Name="Action_LockInteraction" DataType="QuestBehaviourTreeNode_Action_LockInteraction, Assembly-CSharp" />
            <ExtraType Name="Action_LockQuestTarget" DataType="QuestBehaviourTreeNode_Action_LockQuestTarget, Assembly-CSharp" />
            <ExtraType Name="Action_PacifyMinorFaction" DataType="QuestBehaviourTreeNode_Action_PacifyMinorFaction, Assembly-CSharp" />
            <ExtraType Name="Action_RemoveHero" DataType="QuestBehaviourTreeNode_Action_RemoveHero, Assembly-CSharp" />
