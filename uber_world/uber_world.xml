      <DatabasePlugin DataType="Droplist, Assembly-CSharp">
        <FilePath>Quests/Droplists/Droplists[*].xml</FilePath>
        <FilePath>Simulation/Droplists[*].xml</FilePath>
      </DatabasePlugin>

      <DatabasePlugin DataType="QuestDefinition, Assembly-CSharp">
        <Overrides>
          <Override Name="Prerequisites" DataType="Amplitude.Query.Xml.XmlQueryPrerequisites, Assembly-CSharp-firstpass">
            <ExtraType Name="FilterEmpireByDiplomaticRelationState" DataType="QuestFilterEmpireByDiplomaticRelationState, Assembly-CSharp" />
            <ExtraType Name="FilterConstructibleElementByCategory" DataType="QuestFilterConstructibleElementByCategory, Assembly-CSharp" />
            <ExtraType Name="FilterConstructibleElementBySubCategory" DataType="QuestFilterConstructibleElementBySubCategory, Assembly-CSharp" />
            <ExtraType Name="FilterWorldPositionByDistance" DataType="QuestFilterWorldPositionByDistance, Assembly-CSharp" />
            <ExtraType Name="FilterRegionByDistance" DataType="QuestFilterWorldPositionByDistance, Assembly-CSharp" />
            <ExtraType Name="FilterRegionIsOcean" DataType="QuestFilterRegionIsOcean, Assembly-CSharp" />
            <ExtraType Name="FilterRegionIsColonized" DataType="QuestFilterRegionIsColonized, Assembly-CSharp" />
            <ExtraType Name="FilterRegionIsContinent" DataType="QuestFilterRegionIsContinent, Assembly-CSharp" />
            <ExtraType Name="FilterRegionIsOnSameContinent" DataType="QuestFilterRegionIsOnSameContinent, Assembly-CSharp" />
            <ExtraType Name="FilterCityByEmpire" DataType="QuestFilterCityByEmpire, Assembly-CSharp" />
            <ExtraType Name="FilterRegionByEmpire" DataType="QuestFilterRegionByEmpire, Assembly-CSharp" />
          </Override>

          <Override Name="Sorter" DataType="Amplitude.Query.Xml.XmlQuerySorter, Assembly-CSharp-firstpass">
            <ExtraType Name="SortRegionByDistance" DataType="QuestSorterRegionByDistance, Assembly-CSharp" />
            <ExtraType Name="SortPointOfInterestByDistance" DataType="QuestSorterPointOfInterestByDistance, Assembly-CSharp" />
            <ExtraType Name="SortVillageByDistance" DataType="QuestSorterVillageByDistance, Assembly-CSharp" />
            <ExtraType Name="SortCitiesBySimulationProperty" DataType="QuestSorterCitiesBySimulationProperty, Assembly-CSharp" />
          </Override>

          <Override Name="Children" DataType="Amplitude.Unity.AI.BehaviourTree.BehaviourTreeNodeController, Assembly-CSharp-firstpass">

            <!-- Actions -->
            <ExtraType Name="Action_ActivateBooster" DataType="QuestBehaviourTreeNode_Action_ActivateBooster, Assembly-CSharp" />
            <ExtraType Name="Action_AddQuestMarker" DataType="QuestBehaviourTreeNode_Action_AddQuestMarker, Assembly-CSharp" />
            <ExtraType Name="Action_Alert" DataType="QuestBehaviourTreeNode_Action_Alert, Assembly-CSharp" />
            <ExtraType Name="Action_ApplyWorldEffect" DataType="QuestBehaviourTreeNode_Action_ApplyWorldEffect, Assembly-CSharp" />
            <ExtraType Name="Action_Debug-Log" DataType="QuestBehaviourTreeNode_Action_Debug_Log, Assembly-CSharp" />
            <ExtraType Name="Action_DestroyArmy" DataType="QuestBehaviourTreeNode_Action_DestroyArmy, Assembly-CSharp" />
